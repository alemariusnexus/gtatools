INCLUDE("${CMAKE_CURRENT_LIST_DIR}/LibgtaTargets.cmake")


SET(GTA_USE_OPENGL_ES "@GTA_USE_OPENGL_ES@")


GET_PROPERTY(compdefs TARGET gta PROPERTY INTERFACE_COMPILE_DEFINITIONS)

LIST(FIND compdefs "__CMAKE_GTA_DUMMY_EXPORT_IS_INSTALLED" instdefidx1)
LIST(FIND compdefs "-D__CMAKE_GTA_DUMMY_EXPORT_IS_INSTALLED" instdefidx2)

IF(NOT instdefidx1 EQUAL -1  OR  NOT instdefidx2 EQUAL -1)
    SET(INSTALLED ON)
ELSE()
    SET(INSTALLED OFF)
ENDIF()
